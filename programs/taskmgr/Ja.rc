        MENUITEM SEPARATOR
        MENUITEM "上下に並べて表示(&H)",          ID_WINDOWS_TILEHORIZONTALLY
        MENUITEM "左右に並べて表示(&V)",            ID_WINDOWS_TILEVERTICALLY
        MENUITEM "最小化(&M)",                   ID_WINDOWS_MINIMIZE
        MENUITEM "最大化(&X)",                   ID_WINDOWS_MAXIMIZE
        MENUITEM "重ねて表示(&C)",                    ID_WINDOWS_CASCADE
        MENUITEM SEPARATOR
